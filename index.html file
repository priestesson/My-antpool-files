    <script language=javascript>
        function checksinginform() {
            if (document.mainform.username.value == '') {
                $("#loginerror").text("Please type your username!").css("display", 'block');
                document.mainform.username.focus();
                return false;
            }
            if (document.mainform.password.value == '') {
                $("#loginerror").text("Please type your password!").css("display", 'block');
                document.mainform.password.focus();
                return false;
            }
            return true;
        }

        function checkforgotform() {
            if (document.forgotform.email.value == '') {
                showloginerror("Please type your username or email!");
                document.forgotform.email.focus();
                return false;
            }
            return true;
        }

        $(document).ready(function () {
            $("#return_signin_link").click(function (e) {
                showloginform(e);
            });
        });

        function showloginform(e = false) {
            if (e != false) {
                e.preventDefault();
            }
            $("#loginformname").text("SING IN");
            $("#forgot_form_content").hide();
            $("#login_form_content").show();
            $("#loginerror").hide();
        }

        function showloginerror(errortext) {
         $("#loginerror").text(errortext).show();
        }

        function show_forgot_form() {
            $("#loginformname").text("PASSWORD RECOVERY");
            $("#SigninModal").modal('show');
            $("#login_form_content").hide();
            $("#forgot_form_content").show();

        }
    </script>
 




